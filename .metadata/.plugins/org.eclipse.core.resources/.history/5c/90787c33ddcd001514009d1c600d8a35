(function() {
	
	var app = angular.module('askme',[]);
	
	app.controller('regCont',['$scope','$http',function($scope,$http){
		this.username="";
		this.password="";
		this.nickname="";
		this.description="";
		this.imageLink="";
		
		$http({
			method:'POST',
			url:"AskMe/registration",
			transformRequest: function(arr) {
				
				var string = [];
				for( var i in arr )
				{
					string.push(encodeURIComponent(i) + "=" + encodeURIComponent(arr[i]));
				}
				
			}
			
			
		})
		
	}]);
	
	
})();